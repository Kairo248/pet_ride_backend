option_settings:
  aws:elasticbeanstalk:application:environment:
    SPRING_PROFILES_ACTIVE: prod
    RDS_HOSTNAME: `{"Fn::GetAtt": ["AWSEBRDSDatabase", "Endpoint.Address"]}`
    RDS_PORT: `{"Fn::GetAtt": ["AWSEBRDSDatabase", "Endpoint.Port"]}`
    RDS_DB_NAME: petuber
    RDS_USERNAME: admin
    RDS_PASSWORD: ChangeThisPassword123!
    JWT_SECRET: ChangeThisJWTSecretKeyToSomethingVerySecureAndLong
    ALLOWED_ORIGINS: https://your-client-domain.com,https://your-driver-domain.com
  aws:elasticbeanstalk:container:java:
    JVMOptions: -Xmx512m -Xms256m
  aws:elasticbeanstalk:environment:process:default:
    HealthCheckPath: /actuator/health
  aws:elasticbeanstalk:environment:proxy:
    ProxyServer: nginx

